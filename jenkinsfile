pipeline {
  agent {node{label 'master'}}
  stages {
    stage("ping") {
      steps{
        dir("/etc/ansible") {
            sh "pwd"
            sh "ls"
             echo "pingen naar de slave..."
             ansiblePlaybook become: true, installation: 'Ansible', inventory: 'hosts', playbook: 'playbook.yml'
          }
        }
      }
    }
  }
